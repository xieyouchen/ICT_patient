/**index.wxss**/

page {
    height: 100%;
}
.tab{
    position: fixed;
    flex:0 0 325rpx;
}
.main{
    
    position: relative;
    display: flex;
}
.scroll{
    display: flex;
    position: absolute;
}

.container {
    height: 30%;
    margin-bottom: 20rpx;
    /* padding-top: 20px; */
}

.userinfo {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.userinfo-avatar {
    width: 128rpx;
    height: 128rpx;
    margin: 20rpx;
    border-radius: 50%;
}

.userinfo-nickname {
    color: #aaa;
}

.usermotto {
    margin-top: 200px;
}

.first_tab .goods_item {
    display: flex;
    width: 100%;
    border-bottom: 2rpx solid #778899;
}

.first_tab .goods_item .goods_img_wrap {
    min-width: 40%;
    flex: 2;
    display: flex;
    justify-content: center;
    align-items: center;
}

.first_tab .goods_item .goods_img_wrap image {
    width: 100%;
}

.first_tab .goods_item .goods_info_wrap {
    flex: 3;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    margin-left: 20rpx;
}

.first_tab .goods_item .goods_info_wrap .goods_name {
    font-size: 40rpx;
    display: -webkit-box;
    overflow: hidden;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
}

.first_tab .goods_item .goods_info_wrap .goods_price {
    color: #778899;
    font-size: 32rpx;
}
.first_tab .goods_item .goods_info_wrap .icon{
    color:#778899;
    width:40rpx;
    float: left;
    font-size: 35rpx;
  }
  .first_tab .goods_item .goods_info_wrap .like .active{
    color:#000;
    width:40rpx;
    float: left;
    font-size: 35rpx;
  }
  .first_tab .goods_item .goods_info_wrap .collect .active{
    color:#000;
    width:40rpx;
    float: left;
    font-size: 35rpx;
  }
  .first_tab .goods_item .goods_info_wrap .p{
    font-size: 25rpx;
    width:20rpx;
    float: left;
    margin-left: 5rpx;
  }
  .like{
    width:120rpx;
    float: left;
  }
  .collect{
    width:120rpx;
    float: left;
  }
  .comment{
    justify-content: center;
    align-items: center;
    
  }
