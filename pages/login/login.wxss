.center_page{
  display: flex;
  align-items: center;
  height: 100%;
  width: 100%;
  flex-direction: column;
}
.center_top{
  display: flex;
  position: relative;
  height: 200rpx;
  width: 100%;
  background-color: #1818A7;
}
.login_card{
  height: 350rpx;
  width: 650rpx;
  display: flex;
  background-color: #FFFFFF;
  position: absolute;
  left: 50rpx;
  top: 20rpx;
  border-radius: 15rpx;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.non-box{
  height:150rpx;
  margin-bottom: 80rpx;
}
.center_tab{
  display: flex;
  width: 650rpx;
  height: 50%;
  background-color: #FFFFFF;
  border-radius: 15rpx;
  flex-direction: column;
}
.scroll{
  display: flex ;
  width: 650rpx;
  
 margin-top: 10%;
 margin-bottom: 10%;
  border-radius: 15rpx;
  flex-direction: column;
}
.login_button{
  height: 80rpx;
  width:400rpx;
  background-color: #1818A7;
  border-radius: 15rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #ffffff;
}
.login_status{
  height:350rpx;
  width:650rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.tab_top{
  width:100%;
  height:10%;
  border-bottom: 1rpx solid #F8F7EE;
  display: flex ;
  justify-content: center;
}
.one_tab{
  color: #808080;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 100%;
  flex-grow: 1;
  border-right: 1rpx solid #F8F7EE;
}
.one_tab_active{
  color: #000000;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 100%;
  flex-grow: 1;
  border-right: 1rpx solid #F8F7EE;
}

.info{
  height: 150rpx;
  display: flex;
  padding: 40rpx;
  align-items: center;
}
.info_image{
  margin-right: 50rpx;
  max-width: 160rpx;
  max-height: 200rpx;
}
.info_name{
  height:150rpx;
 
}

.check{
  height: 200rpx;
  display: flex;
  align-items: center;
  margin-bottom: 40rpx;
  border-bottom: 1rpx solid #F8F7EE;
}
.check_image{
  height: 200rpx;
  width: 300rpx;
}
.check_text{
  width: 300rpx;
  height: 200rpx;
  display: flex;
  padding-left: 20rpx;
  flex-direction: column;
  box-sizing: border-box;
  text-align: center;
  padding-top: 50rpx;
}
.check_name{
  margin-top: 10rpx;
  height: 80rpx;
  margin-bottom: 40rpx;
  display: flex;
  align-items: center;
}
.check_time{
  height: 24rpx;
  font-size:24rpx;
  color:#A9A9A9;
  margin-top: 20rpx;
}
.choice_mask{
  position: absolute;
  height: 100%;
  width: 750rpx;
  background-color: rgba(200,200,200,0.7);
  top: 0;
  left: 0;
}
.choice_box{
  position: absolute;
  height: 700rpx;
  width: 550rpx;
  left: 100rpx;
  top: 100rpx;
  border-radius: 5%;
  background-color: #F8F7EE;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.tip_text{
  margin-top: 80rpx;
  height: 40rpx;
  width: 100%;
  color: #1818A7;
  font-size: 30rpx;
  display: flex;
  justify-content: center;
  margin-bottom: 100rpx;
}
.choice_tab{
  height:100rpx;
  width:200rpx;
  border: solid 1rpx #1818A7;
  border-radius: 5%;
  background-color: #cfe2f3;
  color: #1818A7;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 100rpx;
  font-size: 28rpx;
}
.no_choice{
  height:100rpx;
  width:200rpx;
  border-radius: 5%;
  background-color: #d2d2d2;
  border: solid 1rpx #d2d2d2;
  color: #000;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 100rpx;
  font-size: 28rpx;
}
.bottom_bar{
  position: fixed;
  height: 100rpx;
  width: 100%;
  background-color: #F8F7EE;
  color: #AFBFCF;
  bottom: 0rpx;
  border-top: solid 1rpx #d2d2d2;
  display: flex;
}
.tab_box{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 187rpx;
}
.tab_icon{
  height: 64rpx;
  width: 64rpx;
  margin-top: 6rpx;
}
.tab_text{
  height: 20rpx;
  font-size: 20rpx;
  display: flex;
  justify-content: center;
}