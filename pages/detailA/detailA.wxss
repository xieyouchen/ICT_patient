/**app.wxss**/
.container {
  position: absolute;
  padding: 10rpx;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  max-height: 60%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 
 
/**index.wxss**/
ec-canvas {
  width: 100%;
  height: 100%;
}
.table {
  border: 0px solid darkgray;
  display: flex;
  position: absolute;
 }
 .tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
 }
 .td {
  width:40%;
  justify-content: center;
  text-align: center;
  font-size: 30rpx;
 }
 .bg-w{
  background: snow;
 }
 .bg-g{
  background: #E6F3F9;
 }
 .bg-h{
  background: #1818F0;
  display: flex;
  width: 100%;
  justify-content: center;
  height: 2.5rem;
  align-items: center;
  color: #fff;
 }
 .th {
  width: 40%;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
  font-size: 30rpx;
 }
 .b{
   margin-top: 400rpx;
 }
 .main{
   display: flex;
   border: 0px solid darkgray;
  /* display: flex; */
  position: absolute;
 }
 .under{
   height: 40%;
   margin-top: 100%;
   width:100%;
   align-items: center;
 }
 